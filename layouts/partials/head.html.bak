{{ $customCSS := .Site.Params.customCSS }}
{{ $customJS := .Site.Params.customJS }}

{{ if $customCSS }}
{{ range $customCSS }}
<link rel="stylesheet" href="{{ . | absURL }}" />
{{ end }}
{{ end }}

{{ if $customJS }}
{{ range $customJS }}
<script src="{{ . | absURL }}"></script>
{{ end }}
{{ end }}
